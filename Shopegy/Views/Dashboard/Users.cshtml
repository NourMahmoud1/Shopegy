@model IEnumerable<BLL.Models.ApplicationUser>
@{
    ViewData["Title"] = "Dashbourd";
    Layout = "_DashbourdLayout";
}

<h2 class="my-3">Normal Users</h2>

<table class="table table-bordered table-striped">
    <thead class="table-dark">
        <tr>
            <th>User Name</th>
            <th>Email</th>
            <th>Address</th>  
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>@user.Address</td>   
            </tr>
        }
    </tbody>
</table>
